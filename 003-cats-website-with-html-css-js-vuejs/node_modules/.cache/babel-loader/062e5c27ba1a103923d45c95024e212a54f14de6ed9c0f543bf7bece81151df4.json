{"ast":null,"code":"import { ref } from 'vue';\nimport logoPath from '@/assets/logo.svg';\nimport openMenuSVG from '@/assets/menu.svg';\nimport closeMenuSVG from '@/assets/close.svg';\nexport default {\n  setup() {\n    const logo = ref({\n      path: logoPath,\n      alt: \"Basic Cat's Food website logo\"\n    });\n    const homeButton = ref({\n      href: 'index.html',\n      text: 'Home'\n    });\n    const openMenuButton = ref({\n      icon: openMenuSVG,\n      title: \"Open menu\",\n      alt: \"Open menu\"\n    });\n    const closeMenuButton = ref({\n      icon: closeMenuSVG,\n      title: \"Close menu\",\n      alt: \"Close menu\"\n    });\n    const menuOption = ref([{\n      text: 'Our Food',\n      href: '#our-food'\n    }, {\n      text: 'Ingredients',\n      href: '#ingredients'\n    }, {\n      text: 'Studies',\n      href: '#studies'\n    }, {\n      text: 'FAQ',\n      href: '#faq'\n    }]);\n    function toggleMenu() {\n      if (!menu.classList.contains('open')) {\n        menu.classList.add('open');\n      } else {\n        menu.classList.remove('open');\n      }\n      const isExpanded = openMenuButton.getAttribute('aria-expanded');\n      openMenuButton.setAttribute(\"aria-expanded\", isExpanded != \"true\");\n    }\n    return {\n      logo,\n      homeButton,\n      openMenuButton,\n      closeMenuButton,\n      menuOption,\n      toggleMenu\n    };\n  }\n};","map":{"version":3,"names":["ref","logoPath","openMenuSVG","closeMenuSVG","setup","logo","path","alt","homeButton","href","text","openMenuButton","icon","title","closeMenuButton","menuOption","toggleMenu","menu","classList","contains","add","remove","isExpanded","getAttribute","setAttribute"],"sources":["/home/ak/vuejs/bootfi-vuejs-road-map/003-cats-website-with-html-css-js-vuejs/src/components/MainHeader.vue"],"sourcesContent":["<template>\n    <header class=\"header\">\n        <div class=\"header-left\">\n            <a href=\"index.html\">\n                <img :src=\"logo.path\" :alt=\"logo.alt\">\n            </a>\n            <span class=\"divider hide-mobile\"></span>\n            <a @click=\"toggleMenu\" :href=\"homeButton.href\" class=\"home hide-mobile\">{{ homeButton.text }}</a>\n        </div>\n\n        <nav aria-label=\"Primary\">\n            <button @click=\"toggleMenu\" :title=\"openMenuButton.title\" class=\"menu-toggle hide-desktop\" aria-expanded=\"false\"\n                aria-controls=\"menu\">\n                <img :src=\"openMenuButton.icon\" :alt=\"openMenuButton.alt\">\n            </button>\n            <ul id=\"menu\" class=\"menu\">\n                <li class=\"hide-desktop\">\n                    <button class=\"menu-close\" :title=\"closeMenuButton.title\" :aria-label=\"closeMenuButton.alt\">\n                        <img :src=\"closeMenuButton.icon\" :alt=\"closeMenuButton.alt\">\n                    </button>\n                </li>\n                <li v-for=\"option in menuOption\" :key=\"option.href\"><a :href=\"option.href\">{{ option.text }}</a></li>\n            </ul>\n        </nav>\n    </header>\n</template>\n\n<script>\nimport { ref } from 'vue';\nimport logoPath from '@/assets/logo.svg';\nimport openMenuSVG from '@/assets/menu.svg'\nimport closeMenuSVG from '@/assets/close.svg'\nexport default {\n    setup() {\n        const logo = ref({\n            path: logoPath,\n            alt: \"Basic Cat's Food website logo\"\n        });\n\n        const homeButton = ref({\n            href: 'index.html',\n            text: 'Home'\n        });\n\n        const openMenuButton = ref({\n            icon: openMenuSVG,\n            title: \"Open menu\",\n            alt: \"Open menu\"\n        });\n\n        const closeMenuButton = ref({\n            icon: closeMenuSVG,\n            title: \"Close menu\",\n            alt: \"Close menu\"\n        });\n\n        const menuOption = ref([\n            { text: 'Our Food', href: '#our-food' },\n            { text: 'Ingredients', href: '#ingredients' },\n            { text: 'Studies', href: '#studies' },\n            { text: 'FAQ', href: '#faq' }\n        ]);\n\n        function toggleMenu() {\n            if (!menu.classList.contains('open')) {\n                menu.classList.add('open');\n            } else {\n                menu.classList.remove('open');\n            }\n            const isExpanded = openMenuButton.getAttribute('aria-expanded');\n            openMenuButton.setAttribute(\"aria-expanded\", isExpanded != \"true\");\n        }\n\n        return {\n            logo,\n            homeButton,\n            openMenuButton,\n            closeMenuButton,\n            menuOption,\n            toggleMenu\n        };\n    }\n};\n</script>\n"],"mappings":"AA4BA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,QAAO,MAAO,mBAAmB;AACxC,OAAOC,WAAU,MAAO,mBAAkB;AAC1C,OAAOC,YAAW,MAAO,oBAAmB;AAC5C,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAMC,IAAG,GAAIL,GAAG,CAAC;MACbM,IAAI,EAAEL,QAAQ;MACdM,GAAG,EAAE;IACT,CAAC,CAAC;IAEF,MAAMC,UAAS,GAAIR,GAAG,CAAC;MACnBS,IAAI,EAAE,YAAY;MAClBC,IAAI,EAAE;IACV,CAAC,CAAC;IAEF,MAAMC,cAAa,GAAIX,GAAG,CAAC;MACvBY,IAAI,EAAEV,WAAW;MACjBW,KAAK,EAAE,WAAW;MAClBN,GAAG,EAAE;IACT,CAAC,CAAC;IAEF,MAAMO,eAAc,GAAId,GAAG,CAAC;MACxBY,IAAI,EAAET,YAAY;MAClBU,KAAK,EAAE,YAAY;MACnBN,GAAG,EAAE;IACT,CAAC,CAAC;IAEF,MAAMQ,UAAS,GAAIf,GAAG,CAAC,CACnB;MAAEU,IAAI,EAAE,UAAU;MAAED,IAAI,EAAE;IAAY,CAAC,EACvC;MAAEC,IAAI,EAAE,aAAa;MAAED,IAAI,EAAE;IAAe,CAAC,EAC7C;MAAEC,IAAI,EAAE,SAAS;MAAED,IAAI,EAAE;IAAW,CAAC,EACrC;MAAEC,IAAI,EAAE,KAAK;MAAED,IAAI,EAAE;IAAO,EAC/B,CAAC;IAEF,SAASO,UAAUA,CAAA,EAAG;MAClB,IAAI,CAACC,IAAI,CAACC,SAAS,CAACC,QAAQ,CAAC,MAAM,CAAC,EAAE;QAClCF,IAAI,CAACC,SAAS,CAACE,GAAG,CAAC,MAAM,CAAC;MAC9B,OAAO;QACHH,IAAI,CAACC,SAAS,CAACG,MAAM,CAAC,MAAM,CAAC;MACjC;MACA,MAAMC,UAAS,GAAIX,cAAc,CAACY,YAAY,CAAC,eAAe,CAAC;MAC/DZ,cAAc,CAACa,YAAY,CAAC,eAAe,EAAEF,UAAS,IAAK,MAAM,CAAC;IACtE;IAEA,OAAO;MACHjB,IAAI;MACJG,UAAU;MACVG,cAAc;MACdG,eAAe;MACfC,UAAU;MACVC;IACJ,CAAC;EACL;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}